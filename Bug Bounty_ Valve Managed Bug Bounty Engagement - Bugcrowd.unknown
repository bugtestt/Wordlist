.rp-portfolio__badge-image-pentesterlab { height: auto; max-width: 128px; }

.rp-portfolio-widget__header { align-items: center; display: flex; justify-content: space-between; }

.rp-portfolio-widget__header .bc-btn { padding-left: 0.25rem; padding-right: 0.25rem; }

