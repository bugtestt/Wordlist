.rp-engagement-card.rp-engagement-card--compact .bc-panel { flex-direction: row; }

.rp-engagement-card.rp-engagement-card--compact .rp-engagement-card__logo-backdrop { flex: 1 1 auto; padding: 0.75rem; }

