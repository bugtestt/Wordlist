.bc-markdown .pl-s .pl-s1, .bc-markdown .pl-smi { color: rgb(51, 51, 51); }

.bc-markdown .pl-ent { color: rgb(99, 163, 92); }

.bc-markdown .pl-k { color: rgb(167, 29, 93); }

.bc-markdown .pl-pds, .bc-markdown .pl-s, .bc-markdown .pl-s .pl-pse .pl-s1, .bc-markdown .pl-sr, .bc-markdown .pl-sr .pl-cce, .bc-markdown .pl-sr .pl-sra, .bc-markdown .pl-sr .pl-sre { color: rgb(24, 54, 145); }

.bc-markdown .pl-v { color: rgb(237, 106, 67); }

.bc-markdown .pl-id { color: rgb(181, 42, 29); }

.bc-markdown .pl-ii { background-color: rgb(181, 42, 29); color: rgb(248, 248, 248); }

.bc-markdown .pl-sr .pl-cce { color: rgb(99, 163, 92); font-weight: 700; }

.bc-markdown .pl-ml { color: rgb(105, 58, 23); }

.bc-markdown .pl-mh, .bc-markdown .pl-mh .pl-en, .bc-markdown .pl-ms { color: rgb(29, 62, 129); font-weight: 700; }

.bc-markdown .pl-mq { color: teal; }

.bc-markdown .pl-mi { color: rgb(51, 51, 51); font-style: italic; }

.bc-markdown .pl-mb { color: rgb(51, 51, 51); font-weight: 700; }

.bc-markdown .pl-md { background-color: rgb(255, 236, 236); color: rgb(189, 44, 0); }

.bc-markdown .pl-mi1 { background-color: rgb(234, 255, 234); color: rgb(85, 165, 50); }

.bc-markdown .pl-mdr { color: rgb(121, 93, 163); font-weight: 700; }

.bc-markdown .pl-mo { color: rgb(29, 62, 129); }

.bc-markdown img { margin-bottom: 1em; max-height: 70rem; max-width: min(80dvw, 100%); }

@media print {
  .bc-markdown img { max-height: none; max-width: 100%; break-after: auto; break-before: auto; break-inside: avoid; }
}

